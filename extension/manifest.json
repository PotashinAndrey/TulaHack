{
    "manifest_version": 3,
    "name": "Reverse auction",
    "version": "1.0",
    "description": "Ссылка «Входящие» и «Написать письмо» снова открываются в той же вкладке!",
    "icons": {
        "128": "logo.png"
    },
    "host_permissions": [
        "https://localhost:7443/"
    ],
    "content_scripts": [
        {
            "matches": [ "<all_urls>" ],
            "js": [ "index.js" ]
        }
    ]
 }